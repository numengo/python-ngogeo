{
  "$id": "https://numengo.org/ngogeo#",
  "type": "object",
  "$comment": "automatically generated for ngogeo",
  "$defs": {
    "coordinates": {
      "type": "object",
      "properties": {
        "CRS": {
          "type": "string",
          "enum": [
            "EPSG:4326",
            "EPSG:2154"
          ]
        }
      },
      "$defs": {
        "HasCrs": {
          "type": "object",
          "abstract": true,
          "properties": {
            "crs": {
              "$ref": "#/$defs/coordinates/properties/CRS"
            }
          }
        },
        "PointCoordinates": {
          "type": "object",
          "properties": {
            "longitude": {
              "type": "number"
            },
            "latitude": {
              "type": "number"
            }
          }
        },
        "Point": {
          "type": "object",
          "extends": [
            "#/$defs/coordinates/$defs/PointCoordinates",
            "#/$defs/coordinates/$defs/HasCrs"
          ],
          "wraps": "shapely.geometry.point.Point",
          "properties": {
            "crs": {
              "$ref": "#/$defs/coordinates/properties/CRS"
            }
          }
        }
      }
    },
    "datasets": {
      "type": "object",
      "$defs": {
        "GeoSeries": {
          "type": "object",
          "extends": [
            "https://numengo.org/ngoschema#/$defs/datasets/$defs/Series"
          ],
          "wraps": "geopandas.geoseries.GeoSeries",
          "properties": {
            "geometry": true
          }
        },
        "GeoDataframe": {
          "type": "object",
          "extends": [
            "https://numengo.org/ngoschema#/$defs/datasets/$defs/Dataframe",
            "#/$defs/datasets/$defs/GeoSeries"
          ],
          "wraps": "geopandas.geodataframe.GeoDataFrame",
          "properties": {
            "crs": {
              "$ref": "#/$defs/coordinates/properties/CRS"
            }
          }
        },
        "GeoDataframeSubset": {
          "type": "object",
          "extends": [
            "https://numengo.org/ngoschema#/$defs/datasets/$defs/DataframeSubset"
          ],
          "aliases": {
            "df": "_dataframe",
            "gdf": "geodataframe"
          },
          "notSerialized": [
            "_dataframe"
          ],
          "readOnly": [
            "_dataframe"
          ],
          "properties": {
            "dataframe": {
              "$ref": "#/$defs/datasets/$defs/GeoDataframe"
            },
            "_dataframe": {
              "$ref": "https://numengo.org/ngoschema#/$defs/datasets/$defs/Dataframe"
            },
            "subset": {
              "$ref": "#/$defs/datasets/$defs/GeoDataframe"
            }
          }
        }
      }
    },
    "infos": {
      "type": "object",
      "$defs": {
        "Language": {
          "type": "object",
          "properties": {
            "alpha_3": {
              "type": "string"
            },
            "name": {
              "type": "string"
            },
            "inverted_name": {
              "type": "string"
            },
            "scope": {
              "type": "string"
            },
            "type": {
              "type": "string"
            }
          }
        },
        "Currency": {
          "type": "object",
          "properties": {
            "alpha3": {
              "type": "string"
            },
            "alpha2": {
              "type": "string"
            },
            "name": {
              "type": "string"
            },
            "numeric": {
              "type": "string"
            }
          }
        },
        "CountryInfo": {
          "type": "object",
          "properties": {
            "ISO": {
              "type": "string"
            },
            "ISO3": {
              "type": "string"
            },
            "ISO-Numeric": {
              "type": "string"
            },
            "fips": {
              "type": "string"
            },
            "Country": {
              "type": "string"
            },
            "Capital": {
              "type": "string"
            },
            "Area(in sq km)": {
              "type": "number"
            },
            "Population": {
              "type": "integer"
            },
            "Continent": {
              "type": "string"
            },
            "tld": {
              "type": "string"
            },
            "CurrencyCode": {
              "type": "string"
            },
            "CurrencyName": {
              "type": "string"
            },
            "Phone": {
              "type": "string"
            },
            "PostalCodeFormat": {
              "type": "string"
            },
            "PostalCodeRegex": {
              "type": "string"
            },
            "Languages": {
              "type": "array",
              "description": "The column 'languages' lists the languages spoken in a country ordered by the number of speakers. The language code is a 'locale' where any two-letter primary-tag is an ISO-639 language abbreviation and any two-letter initial subtag is an ISO-3166 country code.",
              "asString": true
            },
            "geonameid": {
              "type": "integer"
            },
            "neighbours": {
              "type": "array",
              "asString": true
            },
            "EquivalentFipsCode": {
              "type": "integer"
            }
          }
        },
        "PostalInfo": {
          "type": "object",
          "extends": [
            "#/$defs/coordinates/$defs/PointCoordinates"
          ],
          "properties": {
            "country_code": {
              "type": "string"
            },
            "place_name": {
              "type": "string"
            },
            "state_name": {
              "type": "string"
            },
            "state_code": {
              "type": "string"
            },
            "county_name": {
              "type": "string"
            },
            "county_code": {
              "type": "string"
            },
            "community_name": {
              "type": "string"
            },
            "community_code": {
              "type": "string"
            },
            "accuracy": {
              "type": "number"
            }
          }
        },
        "CityInfo": {
          "type": "object",
          "extends": [
            "#/$defs/coordinates/$defs/PointCoordinates"
          ],
          "properties": {
            "geonameid": {
              "type": "integer"
            },
            "asciiname": {
              "type": "string"
            },
            "alternatenames": {
              "type": "array",
              "asString": true
            },
            "featureclass": {
              "type": "string"
            },
            "featurecode": {
              "type": "string"
            },
            "countrycode": {
              "type": "string"
            },
            "countrycode2": {
              "type": "string"
            },
            "admin1code": {
              "type": "string"
            },
            "admin2code": {
              "type": "string"
            },
            "admin3code": {
              "type": "string"
            },
            "admin4code": {
              "type": "string"
            },
            "population": {
              "type": "integer"
            },
            "elevation": {
              "type": "number"
            },
            "dem": {
              "type": "number"
            },
            "timezone": {
              "type": "string"
            },
            "modificationdate": {
              "type": "date"
